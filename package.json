{
  "name": "redux-api-middleware",
  "version": "2.3.0",
  "description": "Redux middleware for calling an API.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "nwb build-web-module",
    "clean": "nwb clean-module",
    "test": "nwb test",
    "test:cover": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "lint": "eslint src test",
    "prepublishOnly": "npm run lint && npm test && npm run clean && npm run build",
    "test:node": "babel-tape-runner test/index.js | tap-spec"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": "agraboso/redux-api-middleware",
  "homepage": "https://github.com/agraboso/redux-api-middleware",
  "keywords": [
    "redux",
    "api",
    "middleware",
    "redux-middleware",
    "flux"
  ],
  "author": {
    "name": "Alberto Garcia-Raboso",
    "email": "agraboso@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "peerDependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.0",
    "eslint-plugin-prettier": "^2.1.2",
    "fetch-mock": "^5.13.1",
    "isomorphic-fetch": "^2.1.1",
    "karma-tap": "^3.1.1",
    "karma-tap-pretty-reporter": "^3.0.5",
    "nwb": "0.20.x",
    "prettier": "^1.5.3",
    "tap-spec": "^4.1.1 ",
    "tape": "^4.8.0"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "es",
    "lib",
    "umd"
  ]
}
